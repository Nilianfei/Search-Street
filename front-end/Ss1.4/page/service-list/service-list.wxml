<!--page/service-list/service-list.wxml-->
<view class='container'>
<loading hidden="{{!loading}}">加载中</loading>
<i-cell-group>
  <block wx:for="{{list}}" wx:key="id">
    <i-cell title="{{item.serviceName}}" value="{{item.servicePrice}}" is-link url="..//?id={{item.shopId}}"></i-cell>
  </block>
</i-cell-group>
</view>
